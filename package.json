    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^2.5.1",
    "sass": "^1.69.3",
    "tailwindcss": "^3.3.5",
    "terser": "^5.14.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.2",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "2.9.1",
    "vitest": "^0.18.1",
    "vue-template-compiler": "^2.7.7",

  },
  "dependencies": {
    "@tsconfig/node18": "^18.2.2",
    "axios": "^1.5.1",
    "element-plus": "^2.5.1",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/jsdom": "^16.2.14",
    "@types/mockjs": "^1.0.9",
    "@types/node": "^18.19.3",
    "@types/nprogress": "^0.2.2",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "tailwindcss": "^3.3.5",
    "terser": "^5.14.2",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "2.9.1",
    "vitest": "^0.18.1",
    "vue-template-compiler": "^2.7.7",
    "vue-tsc": "^1.8.25"
